                        <item>
                         <widget class="QTextEdit" name="textEdit_132">
                          <property name="font">
                           <font>
                            <family>Arial</family>
                            <pointsize>12</pointsize>
                            <weight>10</weight>
                            <italic>false</italic>
                            <bold>false</bold>
                           </font>
                          </property>
                          <property name="frameShape">
                           <enum>QFrame::NoFrame</enum>
