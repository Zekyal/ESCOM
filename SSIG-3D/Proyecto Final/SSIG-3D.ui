                        <string>Campo</string>
                       </attribute>
                       <layout class="QVBoxLayout" name="verticalLayout_108">
                        <item>
                         <widget class="QTextEdit" name="textEdit_94">
                          <property name="font">
                           <font>
                            <family>Arial</family>
                            <pointsize>12</pointsize>
                            <weight>10</weight>
                            <italic>false</italic>
                            <bold>false</bold>
                           </font>
                          </property>
                          <property name="frameShape">
                           <enum>QFrame::NoFrame</enum>
                          </property>
